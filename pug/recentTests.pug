extend default
include common.pug

      
block content
  if(formId)
    -var name = formId;
    -if(tests && tests.length>0) name = tests[0].formName;
    h2 Recently completed #{name} tests
  else
    h2 Recently completed tests:

  table.table
    each test in tests
        tr
            td
              if(test.info.icon)
                img.icon(src=test.info.icon)
            td 
                a(href=`/test/${test._id}`) #{test.formId}
            
            td #{moment(test.insertDate).format()} (#{moment(test.insertDate).fromNow()})

            td
               +short_user(test.user)
            td
                a(href=`/json/test/${test._id}?pretty`) JSON data


