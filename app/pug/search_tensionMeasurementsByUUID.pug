extend default

block vars
  - const page_title = 'Search for Wire Tension Measurements by UUID';

block extrascripts
  script(type = 'text/javascript').
    const dictionary_locations = !{JSON.stringify(dictionary_locations, null, 4)};

  block workscripts
    script(src = '/pages/search_tensionMeasurementsByUUID.js')

block content
  .container-fluid
    .vert-space-x2
      h2 Search for Wire Tension Measurements by UUID

    .vert-space-x2 
      hr

      .vert-space-x1
        p The input box below uses auto-complete - just start typing a component UUID, and any matching options will be displayed.
          br
          | Alternatively, click on the camera icon (to the right of the box) to take a picture of the component's QR code if you have it available.

      .vert-space-x1
        .row
          .col-md-6
            #componentuuidform

    .vert-space-x2
      hr

      h4 Search Results

      .vert-space-x1
        table#results

    .vert-space-x2
